{
  "framework": "nextjs",
  "buildCommand": "cd packages/nextjs && yarn install && yarn build",
  "outputDirectory": "packages/nextjs/.next",
  "rewrites": [
    { "source": "/(.*)", "destination": "/packages/nextjs/$1" }
  ],
  "installCommand": "yarn install",
  "regions": ["sfo1"],
  "git": {
    "deploymentEnabled": {
      "morning-state": true
    }
  }
} 